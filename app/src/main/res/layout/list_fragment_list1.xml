<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:id="@+id/listRegister2"
    >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:orientation="horizontal"
        android:background="#92ced3"
        >
        <Button
            android:id="@+id/btn_ClosetBack"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:background="@mipmap/left_arrow"
            android:layout_centerVertical="true"
            android:layout_marginTop="11.5dp"
            android:layout_marginBottom="11.5dp"
            android:layout_marginLeft="16dp"
            />


        <Button
            android:id="@+id/completeBtn"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginRight="16dp"
            android:layout_marginTop="11.5dp"
            android:layout_marginBottom="11.5dp"
            android:background="@mipmap/check_blue_button"
            android:layout_alignParentRight="true"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#ffffff"
            android:textSize="18sp"
            android:text="리스트 선택"
            android:layout_centerVertical="true"
            android:id="@+id/textView2"
            android:layout_centerHorizontal="true"/>

    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="43dp"
        android:layout_height="469dp"
        android:orientation="vertical"

        >
        <Button
            android:id="@+id/btn_move_list_list1"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:background="@mipmap/shirt_default"
            />
        <Button
            android:id="@+id/btn_move_list_list2"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:background="@mipmap/pants_default"
            />
        <Button
            android:id="@+id/btn_move_list_list3"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:background="@mipmap/jacket_default"
            />
        <Button
            android:id="@+id/btn_move_list_list4"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:background="@mipmap/hat_dafault"
            />
        <Button
            android:id="@+id/btn_move_list_list5"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:background="@mipmap/shoe_default"
            />
        <Button
            android:id="@+id/btn_move_list_list6"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:background="@mipmap/watch_default"
            />
        <Button
            android:id="@+id/btn_move_list_list7"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:background="@mipmap/star_default"
            />
    </LinearLayout>


    <LinearLayout
        android:id="@+id/ll_page_list1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">


            <android.support.v7.widget.RecyclerView
                android:id="@+id/recyclerView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true">


            </android.support.v7.widget.RecyclerView>
        </RelativeLayout>
    </LinearLayout>
    </LinearLayout>

</LinearLayout>